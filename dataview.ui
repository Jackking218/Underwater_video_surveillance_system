               <property name="styleSheet">
                <string notr="true">/* --- 1. 表格整体 --- */
QTableWidget {
    background-color: transparent; /* 背景透明 */
    border: 1px solid rgb(0, 100, 180); /* 外边框 */
    color: white; /* 内容文字白色 */
    gridline-color: transparent; /* 去掉默认网格线，我们自己画 */
    font-size: 14px;
}

/* --- 2. 表头样式 (解决白条问题) --- */
QHeaderView::section {
    background-color: rgba(0, 40, 60, 255); /* 深蓝背景 */
    color: rgb(0, 255, 255); /* 青色文字 */
    padding: 8px;
    border: none;
    border-bottom: 2px solid rgb(0, 200, 255); /* 表头下方的亮线 */
    font-weight: bold;
    font-size: 15px;
}

/* --- 3. 单元格样式 --- */
QTableWidget::item {
    border-bottom: 1px solid rgba(0, 200, 255, 40); /* 每行下面淡淡的线 */
    padding-left: 5px;
}

/* --- 4. 选中行的样式 --- */
QTableWidget::item:selected {
    background-color: rgba(0, 200, 255, 50); /* 选中变亮 */
    color: white;
}

/* --- 5. 左上角那个小方块 --- */
QTableCornerButton::section {
    background-color: rgba(0, 40, 60, 255);
    border: none;
    border-bottom: 2px solid rgb(0, 200, 255);
}</string>
