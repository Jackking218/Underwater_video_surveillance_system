               <property name="styleSheet">
                <string notr="true">/* --- 1. 修复中间的页码 Label (消除白块) --- */
QLabel {
    color: rgb(0, 255, 255); /* 青色文字 */
    font-weight: bold;
    font-size: 16px;
    background-color: transparent; /* 关键：背景透明！ */
    border: none;
    padding: 0 20px; /* 左右留点空隙 */
}

/* --- 2. 美化翻页按钮 (科幻方块) --- */
QPushButton {
    background-color: transparent;
    border: 1px solid rgb(0, 140, 200); /* 青色边框 */
    color: rgb(0, 200, 255); /* 青色文字 */
    border-radius: 4px;
    padding: 5px 15px; /* 稍微撑大一点 */
    font-family: &quot;Microsoft YaHei&quot;;
    font-size: 14px;
}

/* 鼠标悬停 */
QPushButton:hover {
    background-color: rgba(0, 200, 255, 50);
    border: 1px solid rgb(0, 255, 255);
    color: white;
}

/* 鼠标按下 */
QPushButton:pressed {
    background-color: rgb(0, 180, 230);
    color: black;
}

/* 禁用状态 (比如第一页时) */
QPushButton:disabled {
    border: 1px solid rgb(80, 80, 80);
    color: rgb(100, 100, 100);
}</string>
