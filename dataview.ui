             <string notr="true">/* --- 分组框样式 --- */
QGroupBox {
    border: 1px solid rgb(0, 100, 180);
    border-radius: 6px;
    margin-top: 10px; /* 留出标题空间 */
    font-weight: bold;
    color: rgb(0, 255, 255); /* 标题青色 */
}
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 0 5px;
    left: 10px;
}

/* --- 输入框样式 (SpinBox, LineEdit, ComboBox) --- */
QDoubleSpinBox, QSpinBox, QLineEdit, QComboBox {
    background-color: rgba(0, 30, 50, 200);
    border: 1px solid rgb(0, 100, 180);
    color: white;
    padding: 4px;
    border-radius: 2px;
    min-height: 24px;
}
QDoubleSpinBox:hover, QLineEdit:hover, QComboBox:hover {
    border: 1px solid rgb(0, 255, 255);
}

/* --- 按钮样式 (线框风格) --- */
QPushButton {
    background-color: transparent;
    border: 1px solid rgb(0, 160, 230);
    color: rgb(0, 220, 255);
    border-radius: 4px;
    padding: 5px 15px;
    min-height: 24px;
}
QPushButton:hover {
    background-color: rgba(0, 200, 255, 50);
    color: white;
}
QPushButton:pressed {
    background-color: rgb(0, 160, 230);
    color: black;
}

/* --- 日志框 --- */
